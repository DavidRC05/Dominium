
@{
    ViewBag.Title = "Dashboard";
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <title>Focus - Bootstrap Admin Dashboard </title>
    <!-- Favicon icon -->
    <link rel="icon" type="image/png" sizes="16x16" href="./images/favicon.png">
    <link rel="stylesheet" href="~/dashboard/vendor/owl-carousel/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/dashboard/vendor/owl-carousel/css/owl.theme.default.min.css">
    <link href="~/dashboard/vendor/jqvmap/css/jqvmap.min.css" rel="stylesheet">
    <link href="~/dashboard//css/style.css" rel="stylesheet">



</head>

<body>

    <div id="preloader">
        <div class="sk-three-bounce">
            <div class="sk-child sk-bounce1"></div>
            <div class="sk-child sk-bounce2"></div>
            <div class="sk-child sk-bounce3"></div>
        </div>
    </div>
    <div id="main-wrapper">
        <div class="nav-header">
            <a href="@Url.Action("Dashboard", "Admin")">
                <h1>Dominium</h1>
            </a>

            <div class="nav-control">
                <div class="hamburger">
                    <span class="line"></span><span class="line"></span><span class="line"></span>
                </div>
            </div>
        </div>
        <div class="header">
            <div class="header-content">
                <nav class="navbar navbar-expand">
                    <div class="collapse navbar-collapse justify-content-between">
                        <div class="header-left">
                            <div class="search_bar dropdown">
                                <span class="search_icon p-3 c-pointer" data-toggle="dropdown">
                                    <i class="mdi mdi-magnify"></i>
                                </span>
                                <div class="dropdown-menu p-0 m-0">
                                    <form>
                                        <input class="form-control" type="search" placeholder="Search" aria-label="Search">
                                    </form>
                                </div>
                            </div>
                        </div>

                        
                    </div>
                </nav>
            </div>
        </div>
        <div class="quixnav">
            <div class="quixnav-scroll">
                <ul class="metismenu" id="menu">
                    <li class="nav-label first">Dashboard</li>
                    <li>
                        <a href="@Url.Action("Dashboard","Admin"), aria-expanded="false">
                            <i class="icon icon-single-04"></i><span class="nav-text">Menu Principal</span>
                        </a>
                    </li>
                    <li>
                        <a href="@Url.Action("Usuarios","Admin")" aria-expanded="false">
                            <i class="icon icon-app-store"></i><span class="nav-text">Administracion de Usuarios</span>
                        </a>
                    </li>
                    <li>
                        <a href="@Url.Action("Vendedores","Admin")" aria-expanded="false">
                            <i class="icon icon-chart-bar-33"></i><span class="nav-text">Administracion de Vendedores</span>
                        </a>
                        
                    </li>
                    <li>
                        <a href="@Url.Action("Propiedades","Admin")" aria-expanded="false">
                            <i class="icon icon-world-2"></i><span class="nav-text">Administracion de Propiedades</span>
                        </a>
                    </li>
                    <li>
                        <a href="@Url.Action("AgregarPropiedad","Admin")" aria-expanded="false">
                            <i class="icon icon-plug"></i><span class="nav-text">Agregar Propiedad</span>
                        </a>
                    </li>
                </ul>
            </div>


        </div>
        <div class="content-body">
            <div class="container-fluid">
                

            </div>
        </div>
        <div class="footer">
            <div class="copyright">
                <p>Copyright © DOMINIUM</p>
            </div>
        </div>


    </div>

    <script src="~/dashboard/vendor/global/global.min.js"></script>
    <script src="~/dashboard/js/quixnav-init.js"></script>
    <script src="~/dashboard/js/custom.min.js"></script>
    <script src="~/dashboard/vendor/raphael/raphael.min.js"></script>
    <script src="~/dashboard/vendor/morris/morris.min.js"></script>
    <script src="~/dashboard/vendor/circle-progress/circle-progress.min.js"></script>
    <script src="~/dashboard/vendor/chart.js/Chart.bundle.min.js"></script>
    <script src="~/dashboard/vendor/gaugeJS/dist/gauge.min.js"></script>
    <script src="~/dashboard/vendor/flot/jquery.flot.js"></script>
    <script src="~/dashboard/vendor/flot/jquery.flot.resize.js"></script>
    <script src="~/dashboard/vendor/owl-carousel/js/owl.carousel.min.js"></script>
    <script src="~/dashboard/vendor/jqvmap/js/jquery.vmap.min.js"></script>
    <script src="~/dashboard/vendor/jqvmap/js/jquery.vmap.usa.js"></script>
    <script src="~/dashboard/vendor/jquery.counterup/jquery.counterup.min.js"></script>
    <script src="~/dashboard/js/dashboard/dashboard-1.js"></script>

</body>

</html>

